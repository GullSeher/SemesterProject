{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "jsx": "react-native",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "moduleResolution": "node"
    },
    "extends": "expo/tsconfig.base",
    "include": [
      "./*.ts",
      "./*.tsx"
    ],
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js"
    ]
  }
  